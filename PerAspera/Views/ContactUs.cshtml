@using Umbraco.Cms.Web.Common.PublishedModels;
@using PerAspera.Extensions
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.ContactUs>
@using ContentModels = PerAspera.Models.Generated;
@{
    Layout = "_Layout";
}


<div class="breadcrumbs">
    <div class="wrap">
        <ul class="breadcrumbs-list">
            <li class="breadcrumbs-item home">
                <a href="/" class="breadcrumbs-link">
                    Početna <span class="icon font-ico-chevron-right"><span class="sr-only">Icon right</span></span>
                </a>
            </li>
            <li class="breadcrumbs-item">
                <a href="javascript:;" class="breadcrumbs-link">contact</a>
            </li>
        </ul>
    </div>
</div>

@foreach (var item in Model.Content ?? Enumerable.Empty<PublishedElementModel>())
{
    @await Html.NestedContent(item, ViewData)
}